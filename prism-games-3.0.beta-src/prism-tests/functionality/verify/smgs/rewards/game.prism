smg

player p1 [a1] endplayer
player p2 [a2] endplayer

module m1
	s : [0..5];
	t : [0..2];

	[a1] s=0 & t=0 -> (s'=1);
	[a1] s=0 & t=0 -> 0.1 : (t'=1)&(s'=0) + 0.9 : (t'=2)&(s'=0);

	[a2] s=1 & t=0 -> (s'=2);
	[a2] s=1 & t=0 -> 0.2 : (t'=1)&(s'=0) + 0.8 : (t'=2)&(s'=0);

	[a1] s=2 & t=0 -> (s'=3);
	[a1] s=2 & t=0 -> 0.3 : (t'=1)&(s'=0) + 0.7 : (t'=2)&(s'=0);

	[a2] s=3 & t=0 -> (s'=4);
	[a2] s=3 & t=0 -> 0.4 : (t'=1)&(s'=0)+ 0.6 : (t'=2)&(s'=0);

	[a1] s=4 & t=0 -> (s'=5);
	[a1] s=4 & t=0 -> 0.5 : (t'=1)&(s'=0) + 0.5 : (t'=2)&(s'=0);

	[a2] s=5 & t=0 -> (t'=1)&(s'=0);

	[a1] t>0 -> true;

endmodule

rewards
 true : 1;
